model Space {
  id          String    @id @default(uuid())
  title       String
  description String?
  price       Float
  capacity    Int?
  images      String[]  @default([])
  createdAt   DateTime  @default(now())
  ownerId     String
  guidelines  String?
  subTitle    String?
  amenities   Amenity[] @default([])
  bookings    Booking[]
  reviews     Review[]
  owner       User      @relation(fields: [ownerId], references: [id], onDelete: Cascade)
}

enum Amenity {
  WIFI
  AIR_CONDITIONING
  PARKING_AVAILABLE
}
